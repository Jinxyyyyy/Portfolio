import random
from random import sample


def generate_samples(size):
    random_tab = []
    for i in range(size):
      random_number = sample_uniform(10,35)
      random_tab.append(random_number)
    return random_tab


def sample_uniform(a, b):
    return random.randint(a,b)





sample_uniform(10,35)
generate_samples(10)
