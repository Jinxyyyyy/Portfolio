import random
def generate_average(m):
    mean_tab = []
    for x in range(m):
        random_tab = []
        for y in range (5):
            random_number = random.randint(0,1000)
            random_tab.append(random_number)
        mean = sum(random_tab) / len(random_tab)
        mean_tab.append(mean)
    print(mean_tab)

generate_average(4)
